{
  "name": "bein-stream",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker-compose-up": "sudo rm -fr data && docker-compose up --build"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.54.0",
    "@aws-sdk/s3-request-presigner": "^3.54.0",
    "@elastic/elasticsearch": "^7.15.0",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/elasticsearch": "^8.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/microservices": "^8.4.1",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/platform-socket.io": "^8.4.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/websockets": "^8.4.0",
    "@sentry/integrations": "^6.18.2",
    "@sentry/node": "^6.18.2",
    "@types/jsonwebtoken": "^8.5.8",
    "aws-sdk": "^2.1089.0",
    "cache-manager": "^3.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "elasticsearch": "^16.7.2",
    "ioredis": "^4.28.5",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "kafka-retry": "^1.0.7",
    "kafkajs": "^1.16.0",
    "nest-commander": "^2.4.0",
    "nest-winston": "^1.6.2",
    "pg": "^8.7.1",
    "pg-native": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.3",
    "snakecase-keys": "^5.1.2",
    "uuid": "^8.3.2",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@ef-carbon/tspm": "^2.2.5",
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.7",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/testing": "^8.4.0",
    "@sentry/cli": "^1.71.0",
    "@types/cache-manager": "^3.4.3",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.28.8",
    "@types/jest": "27.4.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-spellcheck": "^0.0.19",
    "husky": "^7.0.4",
    "jest": "^27.2.5",
    "lint-staged": "^12.3.5",
    "prettier": "^2.3.2",
    "sequelize": "^6.17.0",
    "sequelize-auto-migrations": "^1.0.3",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.3.5",
    "webpack": "^5.70.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/src/**/**.ts",
      "!**/index.ts",
      "!**/*.dto.ts",
      "!**/*.constant.ts",
      "!**/*.bootstrap.ts",
      "!**/*.constants.ts",
      "!**/*.mock.ts",
      "!**/(mock|test)/*.ts",
      "!**/*.module.ts",
      "!**/*.config.ts",
      "!seed.ts",
      "!**/*.command.ts",
      "!**/cli.ts",
      "!**/configuration.ts",
      "!**/main.ts",
      "!<rootDir>/libs/"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ],
    "moduleNameMapper": {
      "^@app/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@app/sentry(|/.*)$": "<rootDir>/libs/sentry/src/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint",
    "{src,test,wiki}/**/*.{ts,json,md}": "prettier --write"
  }
}
