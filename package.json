{
  "name": "bic-feed-service",
  "version": "1.12.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:worker": "nest build worker",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "DOTENV_CONFIG_PATH=/apps/api/.env nest start --watch",
    "start:api:dev": "DOTENV_CONFIG_PATH=apps/api/.env nest start api --watch",
    "start:worker:dev": "DOTENV_CONFIG_PATH=apps/worker/.env nest start worker --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json",
    "docker-compose-up": "sudo rm -fr data && docker-compose up --build",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/.bin/sequelize migration:generate --name",
    "trigger:command": "TRIGGER_CLI=true node dist/main"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.54.0",
    "@beincom/constants": "^2.1.9",
    "@beincom/domain": "^0.1.7",
    "@beincom/dto": "^2.0.2",
    "@beincom/nest-domain-event": "^0.0.2",
    "@casl/ability": "^5.4.4",
    "@elastic/elasticsearch": "^8.4.0",
    "@metinseylan/nestjs-opentelemetry": "^3.0.0",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/bull": "^0.5.4",
    "@nestjs/common": "^8.4.0",
    "@nestjs/config": "^1.2.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/elasticsearch": "^9.0.0",
    "@nestjs/event-emitter": "^1.0.0",
    "@nestjs/microservices": "^8.4.1",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/schedule": "^1.0.2",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^7.1.16",
    "@nestjs/terminus": "^9.1.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.45.1",
    "@sentry/integrations": "^6.18.2",
    "@sentry/node": "^6.18.2",
    "bull": "^4.8.1",
    "camelcase-keys": "^6.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.0",
    "image-size": "^1.0.1",
    "ioredis": "^5.2.2",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.5",
    "kafkajs": "^1.16.0",
    "nest-commander": "^2.4.0",
    "nestjs-cls": "^3.3.1",
    "nestjs-i18n": "^10.3.1",
    "nestjs-pino": "^3.3.0",
    "node-emoji": "^1.11.0",
    "openai": "^3.3.0",
    "pg": "^8.7.1",
    "pg-native": "^3.0.1",
    "pino-http": "^8.4.0",
    "qs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "remarkable": "^2.0.1",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "semver": "^7.5.1",
    "sequelize-cli": "^6.4.1",
    "sequelize-typescript": "^2.1.3",
    "slate": "^0.82.1",
    "snakecase-keys": "^5.1.2",
    "source-map-support": "^0.5.20",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@automock/jest": "^1.3.1",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-angular": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@ef-carbon/tspm": "^2.2.5",
    "@golevelup/ts-jest": "^0.3.2",
    "@nestjs/cli": "^8.2.1",
    "@nestjs/schematics": "^8.0.7",
    "@nestjs/testing": "^8.4.0",
    "@sentry/cli": "^1.71.0",
    "@types/bull": "^3.15.8",
    "@types/cache-manager": "^3.4.3",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-spellcheck": "^0.0.19",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^27.2.5",
    "lint-staged": "^12.3.5",
    "prettier": "^2.3.2",
    "sequelize": "^6.17.0",
    "sequelize-auto-migrations": "^1.0.3",
    "supertest": "^6.1.3",
    "swagger-ui-express": "^4.3.0",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.3.5",
    "webpack": "^5.70.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "<rootDir>/apps/api/src/**/**.ts",
      "!**/index.ts",
      "!**/*.dto.ts",
      "!**/*.constant.ts",
      "!**/*.model.ts",
      "!**/*.enum.ts",
      "!**/*.bootstrap.ts",
      "!**/*.constants.ts",
      "!**/*.mock.ts",
      "!**/(mock|test)/*.ts",
      "!**/*.module.ts",
      "!**/*.config.ts",
      "!seed.ts",
      "!**/*.command.ts",
      "!**/cli.ts",
      "!**/configuration.ts",
      "!**/main.ts",
      "!<rootDir>/libs/",
      "!**/*.provider.ts",
      "!**/*.value-object.ts",
      "!**/*.entity.ts",
      "!**/*.exception.ts",
      "!**/*.interface.ts",
      "!**/*.decorator.ts",
      "!**/*.interceptor.ts"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/openai(|/.*)$": "<rootDir>/libs/openai/src/$1",
      "^@libs/service(|/.*)$": "<rootDir>/libs/service/src/$1",
      "^@libs/infra(|/.*)$": "<rootDir>/libs/infra/src/$1",
      "^@libs/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@libs/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@api/(|/.*)$": "<rootDir>/api/src/$1"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test}/**/*.ts": "eslint",
    "{src,test,wiki}/**/*.{ts,json,md}": "prettier --write"
  }
}
